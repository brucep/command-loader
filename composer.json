{
    "name": "brucep/command-loader",
    "description": "Add lazily loaded symfony/console commands by default name.",
    "type": "library",
    "license": "MIT",
    "require": {
        "symfony/console": "^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Brucep\\CommandLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {"Brucep\\CommandLoader\\Tests\\": "tests/"}
    },
    "authors": [
        {
            "name": "Bruce Phillips",
            "homepage": "https://github.com/brucep"
        }
    ]
}
